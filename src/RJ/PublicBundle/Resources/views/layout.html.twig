<!DOCTYPE html>
<html>
<head>
    <title>{% block title %}{{ cms.title }}{% endblock %}</title>
    <meta name="description" content="{% block description %}{{ cms.description }}{% endblock %}">
    <meta name="keywords" content="{% block keywords %}{{ cms.keywords }}{% endblock %}">
    <meta name="author" content="Radosław Jaros, {{ cms.contactEmail }}">
    <meta name="copyright" content="Radosław Jaros">
    <meta name="language" content="{{ cms.language }}">
    <meta charset="UTF-8">

    {% stylesheets
    output='css/compiled/vendor_public.css'
    'css/vendor/font-awesome.css'
    'css/vendor/font-pt-sans.less'
    'css/vendor/bootstrap.min.css'
    'css/vendor/bootstrap-theme.min.css' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}

    {#<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap-theme.min.css">#}
    <link rel="stylesheet" href="//code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css">
    <script src="//code.jquery.com/jquery-1.10.2.min.js"></script>
    <script src="//code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>

    {% stylesheets
    output='css/compiled/public.css'
    'css/public.less' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}

    {% block stylesheets %}
    {% endblock %}

    {% block javascripts %}
    {% endblock %}
</head>
<body>
{% block body %}
{% endblock %}
</body>
</html>